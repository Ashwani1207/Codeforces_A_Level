#include<bits/stdc++.h>
using namespace std;
int main()
{
    set<char> mp;
    string s;
    getline(cin,s);
    int l=s.size();
    int count=0;
    for(int i=0;i<l;i++)
    {
        if(s[i]>='a'&&s[i]<='z')
        {
            mp.insert(s[i]);
            count=i;
        }
        
    }
    cout<<mp.size()<<endl;
    
    
    
    return 0;
    
}
