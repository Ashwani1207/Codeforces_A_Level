#include <bits/stdc++.h>
using namespace std;
int main() {
   int xsum=0,ysum=0;,zsum=0;
   int x,y,z;
   int n;
   cin>>n;
   for(int i=0;i<n;i++)
   {
       cin>>x>>y>>z;
       xsum+=x;
       ysum+=y;
       zsum+=z;
   }
   if(!xsum&&!ysum&&!zsum)
   cout<<"YES";
   else
   cout<<"NO";
return 0;
}
