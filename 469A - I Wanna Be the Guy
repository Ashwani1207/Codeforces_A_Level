#include<bits/stdc++.h>
using namespace std;
int main()
{
    set<int> mp;
    int n,p,q;
    cin>>n;
    cin>>p;
    int a[p];
    for(int i=0;i<p;i++)
    {
        cin>>a[i];
        mp.insert(a[i]);
    }
    cin>>q;
    int b[q];
     for(int j=0;j<q;j++)
    {
        cin>>b[j];
        mp.insert(b[j]);
    }
    int s=mp.size();
    
    if(n==s)
    cout<<"I become the guy."<<endl;
    else 
    cout<<"Oh, my keyboard!"<<endl;
   
    
    return 0;
    
}
